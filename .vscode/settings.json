{
    "biome.projects": [
        {
            "folder": "ui"
        }
    ],
    "[svelte]": {
        "editor.defaultFormatter": "biomejs.biome"
    },
    "[typescript]": {
        "editor.defaultFormatter": "biomejs.biome"
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.ts": "explicit",
        "source.organizeImports": "explicit",
    }
}